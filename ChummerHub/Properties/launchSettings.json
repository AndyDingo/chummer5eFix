{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iis": {
      "applicationUrl": "http://localhost/ChummerHub",
      "sslPort": 0
    },
    "iisExpress": {
      "applicationUrl": "https://localhost:5001",
      "sslPort": 5001
    }
  },
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "ChummerHub": {
      "commandName": "Project",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "http://localhost:5000;https://localhost:5001"
    },
    "Docker": {
        "commandName": "Docker",
        "launchBrowser": true,
        "_launchUrl": "{Scheme}://{ServiceHost}",
        "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}",
        "environmentVariables": {
            "_ASPNETCORE_URLS": "https://+:443;http://+:80",
            "_ASPNETCORE_HTTPS_PORT": "44330",
            "ASPNETCORE_HOSTINGSTARTUP__KEYVAULT__CONFIGURATIONENABLED": "false",
            "ConnectionStrings:MasterSqlConnection": "",
            "ConnectionStrings:DefaultConnection": "",
            "ASPNETCORE_HOSTINGSTARTUP__KEYVAULT__CONFIGURATIONVAULT": "https://SINners-0-kv.vault.azure.net"
        },
        "publishAllPorts": true,
        "_httpPort": 10000,
        "useSSL": true,
        "_sslPort": 44330
    }
  }
}
